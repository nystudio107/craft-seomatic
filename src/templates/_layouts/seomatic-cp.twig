{% extends "_layouts/cp" %}

{% from 'seomatic/_includes/macros.twig' import registerJsModules %}

{% block head %}
    {{ parent() }}
    {{ seomatic.manifest.includeCssModule("vendors.css", false) }}
    {{ seomatic.manifest.includeCssModule("styles.css", false) }}
{% endblock %}

{% block foot %}
    {{ parent() }}
    {% set jsModules = [
        'runtime.js',
        'vendors.js',
        'common.js',
        'seomatic.js',
        'vendors~dashboard.js'
    ] %}
    {{ registerJsModules(jsModules) }}
{% endblock %}
